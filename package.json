{
    "name": "webgal",
    "private": true,
    "version": "4.2.8",
    "scripts": {
        "dev": "vite --host 3000",
        "build": "tsc && vite build --base=./",
        "preview": "vite preview",
        "lint": "eslint src/** --fix"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{ts,tsx}": [
            "eslint --cache --fix",
            "git add"
        ],
        "src/**/*.{js,jsx}": [
            "eslint --cache --fix",
            "git add"
        ]
    },
    "dependencies": {
        "@icon-park/react": "^1.3.5",
        "@reduxjs/toolkit": "^1.8.1",
        "@types/lodash": "^4.14.180",
        "angular-expressions": "^1.1.5",
        "axios": "^0.26.1",
        "cloudlogjs": "^1.0.9",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "pixi.js": "^6.3.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-photo-view": "^1.2.2",
        "react-redux": "^8.0.1",
        "sass": "^1.49.9",
        "vite-plugin-package-version": "^1.0.2"
    },
    "devDependencies": {
        "@types/node": "^17.0.23",
        "@types/react": "^17.0.33",
        "@types/react-dom": "^17.0.10",
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "@vitejs/plugin-react": "^1.0.7",
        "eslint": "^8.13.0",
        "eslint-config-alloy": "^4.5.1",
        "eslint-plugin-react": "^7.29.4",
        "lint-staged": "^12.3.7",
        "prettier": "^2.6.2",
        "rollup-plugin-visualizer": "^5.6.0",
        "typescript": "^4.5.4",
        "vite": "^2.8.0"
    }
}
